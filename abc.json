{
  "assets": {
    "type": "command",
    "command": {
      "cmd": [
        "tnpm i",
        "tnpm run build",

        "mkdir -p $BUILD_DEST/dist",
        "cp -r ./build/* $BUILD_DEST/dist",

        "mv ./build/* $BUILD_DEST/",
        "mv ./package.json $BUILD_DEST/",
        "mv ./types/* $BUILD_DEST/"
      ]
    }
  }
}
